%p
  %b Nombre:
  =h @message.name 

%p
  %b Email:
  =h @message.email

%p
  %b Telefono:
  =h @message.phone_number 

%p
  %b Fecha:
  =h @message.created_at.strftime("%d %b, %Y - %H:%M:%S")

%p
  %b IP:
  =h @message.remote_ip

%p
  %b Referrer:
  =h @message.referrer

%p
  %b User-Agent:
  =h @message.user_agent

%p
  %b Mensaje:
  =h @message.body

%p
  %b Urgente contacto:
  =h @message.urgent_contact 


