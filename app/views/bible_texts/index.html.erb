<h1>Listing bible_texts</h1>

<table>
  <tr>
  </tr>

<% for bible_text in @bible_texts %>
  <tr>
    <td><%= link_to 'Show', bible_text %></td>
    <td><%= link_to 'Edit', edit_bible_text_path(bible_text) %></td>
    <td><%= link_to 'Destroy', bible_text, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

